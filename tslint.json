{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "curly": [true, "as-needed"],
        "quotemark": [true, "single", "avoid-escape", "avoid-template"],
        "ordered-imports": false,
        "object-literal-sort-keys": false,
        "only-arrow-functions": false,
        "no-console": false,
        "trailing-comma": false,
        "variable-name": false
    },
    "rulesDirectory": []
}